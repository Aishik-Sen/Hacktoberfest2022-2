/*
for A B C D
    E F G H
    I J K L
    M N O P
*/
#include <iostream>
using namespace std;
int main()
{

   int n;
    cin >>n;
    int i=1;
    while(i<=n){
        int j=1;
        char value='A'+(5*i-1)-4;
        while(j<=n){
        //    char ch=value + i -j; 
            cout<<value<<" ";
            value++;
            j++;
        }
        cout<<endl;
        i++;
    }
       return 0;
}
